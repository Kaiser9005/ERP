    "react": "^18.2.0",
    "react-csv": "^2.2.2",
    "react-dom": "^18.2.0",
    "react-dropzone": "^14.3.5",
    "react-hook-form": "^7.48.2",
    "react-i18next": "^15.1.2",
    "react-plotly.js": "^2.6.0",
    "react-router-dom": "^6.18.0",
    "recharts": "^2.9.3",
    "socket.io-client": "^4.7.2",
